class Solution {
    public int removeDuplicates(int[] nums) {
        int j=0;
        //count=1;
        int k=0;
        for(int i=1;i<nums.length;i++)
        {
            if(nums[j]==nums[i])
            {
                
            }
            else
            {   
                j++;
               nums[j]=nums[i];
                
            }
        }
        return j+1;
        
        
        
    }
}
